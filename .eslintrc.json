{
  "env": {
    "es6": false,
    "browser": true
  },
  "extends": [
    "eslint:recommended"
  ],
  "parserOptions": {
      "ecmaVersion": 5,
      "sourceType": "script",
      "ecmaFeatures": {
          "jsx": false
      }
  },
  "rules": {
      "semi": "error",
      "quotes": ["error", "single"]      
  }
}