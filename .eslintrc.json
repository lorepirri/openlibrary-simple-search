{
  "env": {
    "es6": false,
    "browser": true,
    "jquery": true
  },
  "extends": [
    "eslint:recommended"
  ],
  "parserOptions": {
      "ecmaVersion": 5,
      "sourceType": "script",
      "ecmaFeatures": {
          "jsx": false
      }
  },
  "rules": {
      "semi": "error",
      "quotes": ["error", "single"],
      "indent": ["error" , 2],
      "no-console": ["error", { "allow": ["warn", "error"] }]
  }
}